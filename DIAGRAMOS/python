from graphviz import Digraph

g = Digraph("Priepuolių paieškos taikomosios programos priklausomybių diagrama", format="png")
g.attr(
    label="Priepuolių paieškos taikomosios programos priklausomybių diagrama",
    labelloc="t",
    fontsize="20"
)
g.attr("node", style="filled", fillcolor="lightgreen", shape="box")
g.node("patients_info", "patients_info.py")
g.node("preprocessing", "preprocessing.py")
g.node("segmentation", "segmentation.py")
g.node("labeling_seizures", "labeling_seizures.py")
g.node("balancing", "balancing.py")
g.node("feature_extraction", "feature_extraction.py")
g.node("clasification", "clasification.py")
g.node("postprocessing", "postprocessing.py")
g.node("main", "main.py")
g.node("graphical_user_interface", "graphical_user_interface.py")

g.edge("patients_info"," preprocessing")
g.edge("balancing"," segmentation")
g.edge("balancing"," labeling_seizures")
g.edge("clasification"," postprocessing")
g.edge("clasification"," labeling_seizures")
g.edge("clasification","utils")
g.edge("main","segmentation")
g.edge("main","preprocessing")
g.edge("main","feature_extraction")
g.edge("main","clasification")
g.edge("main","patient_info")
g.edge("main","balancing")
g.edge("main","labeling_seizures")
g.edge("graphical_user_interface","utils")
g.edge("graphical_user_interface","segmentation")
g.edge("graphical_user_interface","preprocessing")
g.edge("graphical_user_interface","feature_extraction")
g.edge("graphical_user_interface","clasification")
g.edge("graphical_user_interface","patient_info")
g.edge("graphical_user_interface","labeling_seizures")


g.render("seizure_detection_dependency_graph", cleanup=True)










